!!! XML
!!!
%html
	%head
		%title= SITE_TITLE
		= javascript_include_tag :application
		= csrf_meta_tag
		= stylesheet_link_tag :application

	%body.bp.two-col

		#container

			#header
				%h1#author Mom's Awesome Comand Center
				%p#author_small (: Love Tyrel :)
				= render 'partials/login'
			= render 'partials/admin_nav'

			= yield

			#footer
				%p.left Copyright &#169; 2013 S.D. O'Donnell
				%p.right Site by Tyrel Richey
			= render 'partials/google'
